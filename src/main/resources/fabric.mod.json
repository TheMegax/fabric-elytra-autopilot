 
{
  "schemaVersion": 1,
  "id": "elytraautopilot",
  "version": "${version}",

  "name": "Elytra AutoPilot",
  "description": "Automatic elytra flight with minimal use of fireworks",
  "authors": [
    "TheMegax",
    "LouisLOurson"
  ],
  "contributors": [
    "401U",
    "Johannes7k75",
    "ubionexd",
    "averycrespi",
    "LQ1234",
    "vadimpronin",
    "HumanoidSandvichDispenser",
    "Cookie_n00b",
    "RazrCraft"
  ],
  "contact": {
    "homepage": "https://github.com/TheMegax/fabric-elytra-autopilot",
